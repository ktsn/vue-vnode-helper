    "babel-preset-power-assert": "^1.0.0",
    "del": "^2.2.0",
    "eslint": "^2.13.1",
    "flow-bin": "^0.31.1",
    "glob": "^7.0.3",
    "gulp": "^3.9.1",
    "gulp-header": "^1.7.1",
    "gulp-rename": "^1.2.2",
